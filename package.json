{
  "name": "knbn-web",
  "version": "0.4.0",
  "description": "A web viewer for the KnBn CLI tool",
  "author": "Matthew Balmer <contact@mattbalmer.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattbalmer/knbn-web"
  },
  "main": "dist/cli/index.js",
  "bin": "dist/cli/index.js",
  "scripts": {
    "build": "tsc && npm run build:client",
    "build:client": "webpack --mode production",
    "dev:client": "webpack --mode development --watch",
    "dev:server": "ts-node-dev src/server/run.ts",
    "start": "ts-node src/server/run.ts"
  },
  "keywords": [
    "knbn",
    "kanban",
    "web",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "knbn-core": "^0.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "classnames-ts": "^1.0.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "jest": "^30.0.3",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "yarn@4.9.2"
}
